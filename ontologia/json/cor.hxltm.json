          "__id": "ontologia.commune.conceptum.codicem",
          "alternativum": {
            "__HXL": "#meta +conceptum +codicem +alternativum",
            "__id": "ontologia.commune.conceptum.codicem.alternativum"
          }
        },
        "dominium": {
          "__HXL": "#item +conceptum +dominium",
          "__id": "ontologia.commune.conceptum.dominium",
          "_TBX": {
            "_id": "DC-489",
            "_desc": "Refers to a location in the corpus—such as a software application\nuser interface, product packaging, oran industrial process—where\nthe term frequently occurs'A sample sentence that contains\nthe term.\n",
            "_nomen": "Subject field",
            "_level": "Concept",
            "_xml": "<descrip type='subjectField'>"
          }
        },
        "meta": {
          "__HXL": "#meta +conceptum",
          "__id": "ontologia.commune.conceptum.meta"
        }
      },
      "rem": {
        "__linguam__": {
          "__HXL": "#item +rem __linguam__",
          "__id": "ontologia.commune.rem.__linguam__",
          "_TBX": {
            "_id": "DC-1823",
            "_nomen": "Term",
            "_desc": "Refers to a location in the corpus—such as a software application\nuser interface, product packaging, oran industrial process—where\nthe term frequently occurs'A sample sentence that contains\nthe term.\n",
            "_xml": "<term>"
          }
        },
        "contextum": {
          "__linguam__": {
            "__HXL": "#item +rem +contextum __linguam__",
            "__id": "ontologia.commune.rem.contextum.__linguam__",
            "_TBX": {
              "_id": "DC-149",
              "_desc": "A sample sentence that contains the term.",
              "_xml": "<descrip type='context'>"
            }
          }
        },
        "definitionem": {
          "__linguam__": {
            "__HXL": "#item +rem +definitionem __linguam__",
            "__id": "ontologia.commune.rem.definitionem.__linguam__",
            "_TBX": {
              "_id": "DC-168",
              "_desc": null,
              "_nomen": "Definition",
              "_level": "Concept, Language",
              "_xml": "<descrip type='definition'>"
            }
          }
        },
        "genus": {
          "__linguam__": {
            "__HXL": "#item +rem +genus __linguam__",
            "__id": "ontologia.commune.rem.genus.__linguam__",
            "_TBX": {
              "_id": "DC-245",
              "_desc": "Picklist, with permissible values as follows:\n  • masculine\n  • feminine\n  • neuter\n  • other\n",
              "_nomen": "Gender",
              "_level": "Term",
              "_xml": "<termNote type='grammaticalGender'>"
            }
          }
        },
        "notam": {
          "__linguam__": {
            "__HXL": "#item +rem +notam __linguam__",
            "__id": "ontologia.commune.rem.notam.__linguam__",
            "_TBX": {
              "_id": "DC-382",
              "_desc": "Any kind of note, such as a usage note, explanation,\nor instruction\n",
              "_nomen": "Note",
              "_level": "Concept, Language, Term",
              "_xml": "<note>"
            }
          }
        },
        "partem_orationis": {
          "__linguam__": {
            "__HXL": "#item +rem +partem_orationis __linguam__",
            "__id": "ontologia.commune.rem.partem_orationis.__linguam__",
            "_TBX": {
              "_id": "DC-396",
              "_nomen": "Part of speech",
              "_desc": "Content type picklist\nPermissible values and their ISOcat PIDs are as follows:\nValue        ISOcat PID\nnoun         www.isocat.org/datcat/DC-1333\nverb         www.isocat.org/datcat/DC-1424\nadjective    www.isocat.org/datcat/DC-1230\nadverb       www.isocat.org/datcat/DC-1232\nproperNoun   www.isocat.org/datcat/DC-384\nother        www.isocat.org/datcat/DC-4336\n\nIn TBX-Default, the data type for part of speech is plainText.\nTBX-Basic's use of picklist is in compliance with TBX-Default\nbecause picklist is more constrained than plainText.\n\nThe other value can be used for terms of the phrase type.\n",
              "_xml": "<termNote type='partOfSpeech'>"
            }
          }
        },
        "typum": {
          "__linguam__": {
            "__HXL": "#item +rem +typum __linguam__",
            "__id": "ontologia.commune.rem.typum.__linguam__",
            "_TBX": {
              "_id": "DC-2677",
              "_nomen": "Term type",
              "_xml": "<termNote type='termType'>",
              "_desc": "Permissible values and their ISOcat PIDs are as follows:\nValue        ISOcat PID\nfullForm     www.isocat.org/datcat/DC-321\nacronym      www.isocat.org/datcat/DC-334\nabbreviation www.isocat.org/datcat/DC-331\nshortForm    www.isocat.org/datcat/DC-332\nvariant      www.isocat.org/datcat/DC-330\nphrase       www.isocat.org/datcat/DC-339\n"
            }
          }
        }
      }
    },
