    "YAML": {
      "archivum_extensionem": ".yml",
      "normam": [
        "https://guides.rubyonrails.org/i18n.html"
      ],
      "situs_interretialis": {
        "referens_officinale": [
          "https://guides.rubyonrails.org/i18n.html"
        ]
      },
      "exemplum": [
        "https://github.com/i18next/react-i18next/blob/master/example/react/public/locales/de/translation.json"
      ]
    }
